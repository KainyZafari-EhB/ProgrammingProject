{
  "expo": {
    "name": "ProgrammingProject",
    "slug": "ProgrammingProject",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Deze app heeft toegang tot de camera nodig.",
        "NSLocationWhenInUseUsageDescription": "Deze app gebruikt je locatie om X te doen.",
        "NSPhotoLibraryUsageDescription": "Deze app heeft toegang tot je foto's nodig."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
